  font-family: "PingFang SC";
  margin-bottom: 100rpx;
  line-height: 52rpx;
}
.edit{
  position: absolute;
  top:0;
  right: 0;
  width:235rpx;
  height:235rpx
}
.lineHeng{
  margin-top: 82rpx;
  height: 40rpx;
  background:linear-gradient(#f9f9f9,#fcfcfc);
